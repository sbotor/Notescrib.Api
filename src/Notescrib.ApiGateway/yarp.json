{
  "Routes": {
    "auth": {
      "ClusterId": "identity",
      "Match": {
        "Path": "/api/identity/auth",
        "Hosts": [ "localhost" ],
        "Methods": [ "POST" ]
      },
      "Transforms": [
        { "PathSet": "/api/auth" }
      ]
    },
    "users": {
      "ClusterId": "identity",
      "AuthorizationPolicy": "Default",
      "Match": {
        "Path": "/api/identity/users/{**rest}",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET", "POST", "DELETE" ]
      },
      "Transforms": [
        { "PathPattern": "/api/users/{**rest}" }
      ]
    },
    "create_user": {
      "ClusterId": "identity",
      "Match": {
        "Path": "/api/identity/users",
        "Hosts": [ "localhost" ],
        "Methods": [ "POST" ]
      },
      "Transforms": [
        { "PathSet": "/api/users" }
      ]
    },
    "activate_account": {
      "ClusterId": "identity",
      "Match": {
        "Path": "/api/identity/users/{id}/activate",
        "Hosts": [ "localhost" ],
        "Methods": [ "POST" ]
      },
      "Transforms": [
        { "PathPattern": "/api/users/{id}/activate" }
      ]
    },
    "init_password_reset": {
      "ClusterId": "identity",
      "Match": {
        "Path": "/api/identity/users/password",
        "Hosts": [ "localhost" ],
        "Methods": [ "POST" ]
      },
      "Transforms": [
        { "PathPattern": "/api/users/password" }
      ]
    },
    "reset_password": {
      "ClusterId": "identity",
      "Match": {
        "Path": "/api/identity/users/{id}/password",
        "Hosts": [ "localhost" ],
        "Methods": [ "PUT" ]
      },
      "Transforms": [
        { "PathPattern": "/api/users/{id}/password" }
      ]
    },
    "identity_health_check": {
      "ClusterId": "identity",
      "Match": {
        "Path": "/health/identity",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET" ]
      },
      "Transforms": [
        { "PathSet": "/health" }
      ]
    },
    "workspaces": {
      "ClusterId": "notes",
      "AuthorizationPolicy": "Default",
      "Match": {
        "Path": "/api/workspaces/{**rest}",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET", "POST", "PUT", "DELETE" ]
      }
    },
    "folders": {
      "ClusterId": "notes",
      "AuthorizationPolicy": "Default",
      "Match": {
        "Path": "/api/folders/{**rest}",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET", "POST", "PUT", "DELETE" ]
      }
    },
    "notes": {
      "ClusterId": "notes",
      "AuthorizationPolicy": "Default",
      "Match": {
        "Path": "/api/notes/{**rest}",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET", "POST", "PUT", "DELETE" ]
      }
    },
    "search_notes": {
      "ClusterId": "notes",
      "Match": {
        "Path": "/api/notes",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET" ]
      }
    },
    "get_note_details": {
      "ClusterId": "notes",
      "Match": {
        "Path": "/api/notes/{id}",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET" ]
      }
    },
    "templates": {
      "ClusterId": "notes",
      "AuthorizationPolicy": "Default",
      "Match": {
        "Path": "/api/templates/{**rest}",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET", "POST", "PUT", "DELETE" ]
      }
    },
    "notes_health_check": {
      "ClusterId": "notes",
      "Match": {
        "Path": "/health/notes",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET" ]
      },
      "Transforms": [
        { "PathSet": "/health" }
      ]
    },
    "emails_health_check": {
      "ClusterId": "emails",
      "Match": {
        "Path": "/health/emails",
        "Hosts": [ "localhost" ],
        "Methods": [ "GET" ]
      },
      "Transforms": [
        { "PathSet": "/health" }
      ]
    }
  },
  "Clusters": {
    "identity": {
      "Destinations": {
        "identity_api": {
          "Address": "https://localhost:5001"
        }
      }
    },
    "notes": {
      "Destinations": {
        "notes_api": {
          "Address": "https://localhost:5002"
        }
      }
    },
    "emails": {
      "Destinations": {
        "emails_api": {
          "Address": "https://localhost:5003"
        }
      }
    }
  }
}